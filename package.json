{
    "private": true,
    "scripts": {
        "serve": "vite",
        "serve:on:docker": "vite --host 0.0.0.0",
        "start": "concurrently \"npm:serve:on:docker\" \"npm:backend\"",
        "start:cy": "concurrently \"npm:serve:on:docker\" \"npm:backend:cy\"",
        "start:preview": "concurrently \"npm:build:preview\" \"npm:backend:preview\"",
        "build": "vite build",
        "build:preview": "vite build --mode preview",
        "cy": "cypress open --browser chrome",
        "lint": "npx eslint .",
        "backend": "php -d variables_order=EGPCS artisan serve --host=0.0.0.0 --port=80",
        "backend:cy": "php -d variables_order=EGPCS artisan serve --env=cypress --host=0.0.0.0 --port=80",
        "backend:preview": "php -d variables_order=EGPCS artisan serve --env=preview --host=0.0.0.0 --port=80"
    },
    "devDependencies": {
        "@iconify-json/mdi": "^1.0.16",
        "@inertiajs/inertia": "^0.10.0",
        "@inertiajs/inertia-vue3": "^0.5.1",
        "@inertiajs/progress": "^0.2.6",
        "@kyvg/vue3-notification": "^2.3.4",
        "@quasar/extras": "^1.12.5",
        "@quasar/vite-plugin": "^1.0.5",
        "@tailwindcss/forms": "^0.4.0",
        "@vitejs/plugin-vue": "^2.1.0",
        "@vue/compiler-sfc": "^3.0.5",
        "autoprefixer": "^10.2.4",
        "axios": "^0.21",
        "concurrently": "^7.2.1",
        "dotenv": "^14.3.2",
        "eslint-config-standard": "^16.0.3",
        "eslint-plugin-cypress": "^2.12.1",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^5.2.0",
        "eslint-plugin-vue": "^8.4.0",
        "lodash": "^4.17.19",
        "postcss": "^8.2.13",
        "postcss-extend-rule": "^4.0.0",
        "postcss-import": "^14.0.1",
        "postcss-nesting": "^10.1.2",
        "quasar": "^2.5.5",
        "sass": "^1.49.9",
        "tailwindcss": "^3.0.18",
        "unplugin-auto-import": "^0.5.11",
        "unplugin-icons": "^0.13.0",
        "unplugin-vue-components": "^0.17.15",
        "vite": "^2.7.13",
        "vue": "^3.0.5"
    }
}
